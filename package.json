{
  "name": "cocktail",
  "version": "1.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "prebuild": "npx --yes expo-doctor@latest && expo prebuild",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "yarn test:lint && yarn test:types && yarn test:jest:ci",
    "test:types": "tsc",
    "test:lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0 --cache --cache-strategy content --cache-location .cache/eslint.json",
    "test:jest": "jest --watch --runInBand --bail",
    "test:jest:ci": "jest",
    "find-dead-code": "ts-prune --ignore '__mocks__/*|src/testing/*|metro.config.js|.*.d.ts'"
  },
  "dependencies": {
    "@formatjs/intl-listformat": "^7.5.5",
    "@formatjs/intl-locale": "^3.4.5",
    "@shopify/flash-list": "^1.6.4",
    "expo": "~50.0.8",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.9",
    "expo-localization": "~14.8.3",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "luxon": "^3.4.4",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^15.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@bam.tech/eslint-plugin": "^6.0.0",
    "@sucrase/jest-plugin": "^3.0.0",
    "@testing-library/react-native": "^12.4.4",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.2",
    "jest-fail-on-console": "^3.1.2",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.2.0",
    "sucrase": "^3.35.0",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.4.3"
  },
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
